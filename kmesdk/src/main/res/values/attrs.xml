<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QuickPollProgressBar">
        <attr name="currentProgress" format="integer" />
        <attr name="progressMax" format="integer" />
        <attr name="progressReachedColor" format="color" />
        <attr name="progressReachedBarHeight" format="dimension" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
        <attr name="progressTextOffset" format="dimension" />
        <attr name="leftIcon" format="integer" />
    </declare-styleable>

    <declare-styleable name="KmeRatingBar">
        <attr name="filledStarWidth" format="dimension" />
        <attr name="filledStarHeight" format="dimension" />
        <attr name="emptyStarWidth" format="dimension" />
        <attr name="emptyStarHeight" format="dimension" />
        <attr name="starSpacing" format="dimension" />
        <attr name="enabled" format="boolean" />
        <attr name="rating" format="integer" />
        <attr name="numStars" format="integer" />
        <attr name="drawableFilledRate" format="reference" />
        <attr name="drawableEmptyRate" format="reference" />
    </declare-styleable>

    <declare-styleable name="ZoomLayout">
        <attr name="min_zoom" format="float"/>
        <attr name="max_zoom" format="float"/>
        <attr name="double_click_zoom" format="float"/>
    </declare-styleable>

</resources>